# 数据库配置
DATABASE_URL=sqlite:///./engineering_rag.db

# 系统配置
MAX_UPLOAD_SIZE=10485760  # 10MB
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=./logs/app.log 